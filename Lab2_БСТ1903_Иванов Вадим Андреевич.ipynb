    "def Queens(n):\n",
    "    if n % 6 == 2:\n",
    "        even = list(range(1, n, 2))\n",
    "        odd = list(range(2, n, 2))\n",
    "        odd[1] = 0\n",
    "        odd.append(4)\n",
    "    elif n % 6 == 3:\n",
    "        even = list(range(3, n, 2))\n",
    "        odd = list(range(4, n, 2))\n",
    "        even.append(1)\n",
    "        odd.append(0)\n",
    "        odd.append(2)\n",
    "    else:\n",
    "        even = list(range(1, n, 2))\n",
    "        odd = list(range(0, n, 2))\n",
    "    rez = even + odd\n",
    "    row = [\".\"] * n\n",
    "    for i in range(n):\n",
    "        j = rez[i]\n",
    "        row[j] = \"Ф\"\n",
    "        print(* row)\n",
    "        row[j] = \".\"\n",
    "    i = n - 1\n",
    "    print(\"\\n\")\n",
    "    while i >= 0:\n",
    "        j = rez[i]\n",
    "        row[j] = \"Ф\"\n",
    "        print(* row)\n",
    "        row[j] = \".\"\n",
    "        i -= 1\n",
    "\n",
